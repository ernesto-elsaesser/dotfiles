        {
            "command": "editmenu:clear-current",
            "keys": [
                "F"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "kernelmenu:restart-and-clear",
            "keys": [
                "G"
            ],
            "selector": "[data-jp-kernel-user]:focus"
        },
        {
            "command": "notebook:enter-edit-mode",
            "keys": [
