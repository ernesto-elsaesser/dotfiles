{
    "shortcuts": [
        {
            "command": "editmenu:go-to-line",
            "keys": [
                "Ctrl G"
            ],
            "selector": ".jp-FileEditor .jp-CodeMirrorEditor"
        },
        {
            "command": "Collapsible_Headings:Toggle_Collapse",
            "keys": [
                "Q"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "notebook:run-cell",
            "keys": [
                "W"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "notebook:run-cell-and-select-next",
            "keys": [
                "E"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "notebook:run-cell-and-insert-below",
            "keys": [
                "S"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "editmenu:clear-current",
            "keys": [
                "F"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "kernelmenu:restart-and-clear",
            "keys": [
                "G"
            ],
            "selector": "[data-jp-kernel-user]:focus"
        },
        {
            "command": "notebook:enter-edit-mode",
            "keys": [
                "L"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "application:toggle-left-area",
            "keys": [
                "Accel B"
            ],
            "selector": "body",
            "disabled": true
        }
    ]
}
