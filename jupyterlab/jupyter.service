[Unit]
Description=Jupyter
After=syslog.target network.target

[Service]
Type=simple
PIDFile=/run/jupyter.pid
Environment="PATH=/home/ee/anaconda3/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"
WorkingDirectory=/home/ee
ExecStart=/home/ee/anaconda3/bin/jupyter lab
User=ee
Group=ee

Restart=on-failure
RestartSec=10

[Install]
WantedBy=multi-user.target
