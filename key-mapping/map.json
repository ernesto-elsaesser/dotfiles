{
  "title": "Maps",
  "rules": [
    {
      "description": "Fn + H = Backspace",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            },
            "key_code": "h"
          },
          "to": [
            {
              "repeat": true,
              "key_code": "delete_or_backspace"
            }
          ]
        }
      ]
    },
    {
      "description": "Fn + J = Enter",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            },
            "key_code": "j"
          },
          "to": [
            {
              "repeat": true,
              "key_code": "return_or_enter"
            }
          ]
        }
      ]
    },
    {
      "description": "Shift + OE = Escape",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": [
                "left_shift"
              ]
            },
            "key_code": "semicolon"
          },
          "to": [
            {
              "repeat": false,
              "key_code": "escape"
            }
          ]
        }
      ]
    }
  ]
}
